{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "imports": true,
    "ARGV": true,
    "print": true,
    "printerr": true
  },
  "rules": {
    "no-case-declarations": 0,
    "no-fallthrough": 0,
    "no-console": 0,
    "semi": [2, "always"],
    "no-extra-semi": 0
  }
}
